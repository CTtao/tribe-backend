<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ct.tribe.mapper.TribeBuildingMapper">

    <resultMap id="BaseResultMap" type="com.ct.tribe.domain.TribeBuilding">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="buildingName" column="building_name" jdbcType="VARCHAR"/>
            <result property="buildingDescription" column="building_description" jdbcType="VARCHAR"/>
            <result property="isDelete" column="is_delete" jdbcType="TINYINT"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="buildingScore" column="building_score" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,building_name,building_description,
        is_delete,create_time,building_score
    </sql>
</mapper>
